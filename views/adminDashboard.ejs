<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Dashboard</title>
    <link rel="stylesheet" href="/styles/adminDashboard.css">
</head>
<body>
    <h1>Admin Dashboard</h1>
    <a href="/logout">Logout</a>

    <!-- Filter Form for College Name -->
    <form method="get" action="/admin-dash">
        <label for="college">Filter by College:</label>
        <select name="college" id="college">
            <option value="">All Colleges</option>
            <% colleges.forEach(college => { %>
                <option value="<%= college %>" <%= collegeFilter === college ? 'selected' : '' %>><%= college %></option>
            <% }) %>
        </select>
        <button type="submit">Apply Filter</button>
    </form>

    <!-- Filter Form for Event Name -->
    <form method="get" action="/admin-dash">
        <label for="event">Filter by Event:</label>
        <select name="event" id="event" onchange="toggleAllUsersDropdown(this.value)">
            <option value="">All Events</option>
            <% events.forEach(event => { %>
                <option value="<%= event.name %>" <%= eventFilter === event.name ? 'selected' : '' %>><%= event.name %></option>
            <% }) %>
            <option value="allUsers" <%= eventFilter === 'allUsers' ? 'selected' : '' %>>All Registered Users</option>
        </select>
        <button type="submit">Apply Filter</button>
    </form>

    <!-- Display College Student Count
    <div>
        <h2>Total Students from <%= collegeFilter || 'All Colleges' %>: <%= collegeStudentCount %></h2>
    </div> -->

    <!-- Display Event Details -->
    <% if (eventFilter !== 'allUsers') { %>
        <% eventDetails.forEach(event => { %>
            <div class="event">
                <div class="event-header"><%= event.eventName %></div>
                <div class="event-details">
                    <table>
                        <thead>
                            <tr>
                                <th>Serial No</th>
                                <th>Team ID</th>
                                <th>Student Names</th>
                                <th>Emails</th>
                                <th>Department</th>
                                <th>College Name</th>
                                <th>Contact Number</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% event.teams.forEach((team, index) => { %>
                                <% team.membersDetails.forEach((member, memberIndex) => { %>
                                    <tr>
                                        <% if (memberIndex === 0) { %>
                                            <td rowspan="<%= team.membersDetails.length %>"><%= index + 1 %></td>
                                            <td rowspan="<%= team.membersDetails.length %>"><%= team.teamId %></td>
                                        <% } %>
                                        <td><%= member.studentName %></td>
                                        <td><%= member.email %></td>
                                        <td><%= member.stream ? member.degree + ' - ' + (member.stream || '') +'- Year- '+(member.yearOfStudy || '') : 'N/A' %></td>
                                        <td><%= member.collegeName %></td>
                                        <td><%= member.contactNumber %></td>
                                    </tr>
                                <% }) %>
                            <% }) %>
                        </tbody>
                    </table>
                    <div class="winners-section">
                        <h3>Winners</h3>
                        <% if (event.winners.length > 0) { %>
                            <ul>
                                <% event.winners.forEach(winner => { %>
                                    <li><%= winner.winnerTeamId %> - <%= winner.winnerParticipants %> (Winner)</li>
                                    <li><%= winner.runnerUpTeamId %> - <%= winner.runnerUpParticipants %> (Runner-Up)</li>
                                <% }) %>
                            </ul>
                        <% } else { %>
                            <p>No winners for this event</p>
                        <% } %>
                    </div>
                </div>
            </div>
        <% }) %>
    <% } else { %>
        <!-- Display All Registered Users when "All Registered Users" is selected -->
        <div id="user-details">
            <h2>All Registered Users</h2>
            <table>
                <thead>
                    <tr>
                        <th>Serial No</th>
                        <th>Student Name</th>
                        <th>College</th>
                        <th>Degree</th>
                        <th>Email</th>
                        <th>Contact Number</th>
                    </tr>
                </thead>
                <tbody>
                    <% allUsers.forEach((user, index) => { %>
                        <tr>
                            <td><%= index + 1 %></td>
                            <td><%= user.studentName %></td>
                            <td><%= user.collegeName %></td>
                            <td><%= user.degree + ' - '+(user.yearOfStudy || '')+'Year'%></td>
                            <td><%= user.email %></td>
                            <td><%= user.contactNumber %></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    <% } %>

    <script>
        // Function to toggle the visibility of the "All Registered Users" dropdown section
        function toggleAllUsersDropdown(selectedValue) {
            const userDetails = document.getElementById('user-details');
            if (selectedValue === 'allUsers') {
                userDetails.style.display = 'block';
            } else {
                userDetails.style.display = 'none';
            }
        }

        // Initial setup to handle the visibility of the "All Registered Users" section
        document.addEventListener('DOMContentLoaded', () => {
            toggleAllUsersDropdown('<%= eventFilter %>');
        });

        // Toggle event details
        document.querySelectorAll('.event-header').forEach(header => {
            header.addEventListener('click', function() {
                const details = this.nextElementSibling;
                details.style.display = details.style.display === 'block' ? 'none' : 'block';
            });
        });
    </script>
</body>
</html>
